<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">
    <context:component-scan base-package="com.ffideal.spring" use-default-filters="false"></context:component-scan>

    <!-- 注册组件 -->
    <bean id="employee" class="com.ffideal.spring.entity.Employee">
        <property name="name" value="法外狂徒张三"></property>
        <property name="age" value="18"></property>
        <property name="gender" value="男"></property>
    </bean>

</beans>